AddKeysToAgent yes

Host *
  AddKeysToAgent yes
  ForwardAgent Yes
  SetEnv TERM=xterm-256color

Host j-sh.de
  SetEnv TERM=xterm-256color
  User jk
  ForwardAgent Yes

Host github.com
    Hostname ssh.github.com
    Port 443
    User git

Host master
  HostName 127.0.0.1
  User vagrant
  Port 2201
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile /home/jk/code/dhbw/vagrant-hadoop-cluster/.vagrant/machines/master/virtualbox/private_key
  IdentitiesOnly yes
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no

Host birdstrike-vm
  HostName 10.50.15.80
  User birdstrike
  PreferredAuthentications password
  PubkeyAuthentication no
  PasswordAuthentication yes
  ForwardAgent yes
  SetEnv TERM=xterm-256color

Host birdstrike-ai
  HostName 10.50.15.140
  User birdstrike
  PreferredAuthentications password
  PubkeyAuthentication no
  PasswordAuthentication yes
  ForwardAgent yes
  SetEnv TERM=xterm-256color

Host usbproxy
  HostName usbproxy.local
  User jk
  SetEnv TERM=xterm-256color
  IdentityFile ~/.ssh/usbproxy_ed25519
